{
    "name": "Crawl Template",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "用于定义抓取模板",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "html/popup/popup.html"
    },
    "options_page" : "html/options.html",
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/",
        "storage",
        "cookies"
    ],
    "background": {
        "scripts": [
            "/js/lib/jquery.js",
            "/js/background.js"
        ]
    },
    "web_accessible_resources": [
        "html/iframe.html"
        ],
    "icons": {
        "128": "icon128.png"
    }
}
